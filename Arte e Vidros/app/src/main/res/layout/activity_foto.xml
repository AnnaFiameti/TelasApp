<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BDC4F0"
    tools:context=".foto">

    <Button
        android:id="@+id/button80"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="170dp"
        android:layout_marginEnd="170dp"
        android:layout_marginBottom="4dp"
        android:background="@drawable/home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="86dp"
        android:layout_height="39dp"
        android:layout_marginTop="44dp"
        android:text="Foto"
        android:textColor="@android:color/black"
        android:textSize="30sp"
        android:textStyle="bold|italic"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="112dp" />

    <Button
        android:id="@+id/button74"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="320dp"
        android:background="@drawable/perfil"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView35"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="93dp"
        android:layout_marginTop="77dp"
        android:layout_marginEnd="93dp"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/button75"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView10"
        app:srcCompat="@drawable/novafoto" />

    <Button
        android:id="@+id/button75"
        android:layout_width="107dp"
        android:layout_height="47dp"
        android:background="@android:color/white"
        android:text="Fazer Upload"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="401dp" />

    <Button
        android:id="@+id/button76"
        android:layout_width="107dp"
        android:layout_height="47dp"
        android:background="@android:color/darker_gray"
        android:text="Tirar Foto"
        tools:layout_editor_absoluteX="211dp"
        tools:layout_editor_absoluteY="401dp" />

    <Button
        android:id="@+id/button77"
        android:layout_width="120dp"
        android:layout_height="53dp"
        android:layout_marginStart="50dp"
        android:layout_marginEnd="70dp"
        android:background="@android:color/holo_red_dark"
        android:text="Cancelar"
        app:layout_constraintEnd_toStartOf="@+id/button78"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="565dp" />

    <Button
        android:id="@+id/button78"
        android:layout_width="120dp"
        android:layout_height="53dp"
        android:layout_marginEnd="50dp"
        android:background="@android:color/holo_green_dark"
        android:text="Selecionar"
        app:layout_constraintEnd_toEndOf="parent"
        tools:layout_editor_absoluteY="565dp" />
</androidx.constraintlayout.widget.ConstraintLayout>