<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BDC4F0"
    tools:context=".Telhados">

    <Button
        android:id="@+id/button58"
        android:layout_width="70dp"
        android:layout_height="70dp"
        android:layout_marginStart="170dp"
        android:layout_marginEnd="170dp"
        android:layout_marginBottom="4dp"
        android:background="@drawable/home"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button56"
        android:layout_width="170dp"
        android:layout_height="38dp"
        android:layout_marginStart="62dp"
        android:layout_marginEnd="179dp"
        android:layout_marginBottom="30dp"
        android:background="@android:color/holo_blue_dark"
        android:text="Com Tubo"
        app:layout_constraintBottom_toTopOf="@+id/imageView28"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button57"
        android:layout_width="170dp"
        android:layout_height="38dp"
        android:layout_marginStart="62dp"
        android:layout_marginEnd="179dp"
        android:layout_marginBottom="30dp"
        android:background="@android:color/holo_blue_dark"
        android:text="Com Tubo"
        app:layout_constraintBottom_toTopOf="@+id/imageView29"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/button55"
        android:layout_width="90dp"
        android:layout_height="90dp"
        android:layout_marginTop="16dp"
        android:background="@drawable/paginas"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="-36dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="86dp"
        android:layout_height="27dp"
        android:layout_marginTop="50dp"
        android:text="Telhado"
        android:textColor="@android:color/black"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="69dp" />

    <ImageView
        android:id="@+id/imageView28"
        android:layout_width="111dp"
        android:layout_height="92dp"
        android:scaleType="centerCrop"
        app:srcCompat="@drawable/comtubo"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="227dp" />

    <ImageView
        android:id="@+id/imageView29"
        android:layout_width="111dp"
        android:layout_height="92dp"
        app:srcCompat="@drawable/telado"
        tools:layout_editor_absoluteX="93dp"
        tools:layout_editor_absoluteY="428dp" />
</androidx.constraintlayout.widget.ConstraintLayout>